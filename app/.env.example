# Admin password (client-side gate; not secure for real production)
VITE_ADMIN_PASSWORD=Password

# Cloudinary configuration for unsigned uploads
VITE_CLOUDINARY_CLOUD_NAME=
VITE_CLOUDINARY_UPLOAD_PRESET=

# Upload target: 'local' (Node server) or 'cloudinary'
VITE_UPLOAD_TARGET=local

# Toggle Admin visibility (true/false)
VITE_ADMIN_ENABLED=false

# Optional: owner setup token to protect /owner-setup route
VITE_OWNER_TOKEN=
